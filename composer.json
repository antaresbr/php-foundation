{
    "name": "antaresbr/foundation",
    "description": "A PHP foundation for antaresbr packages",
    "homepage": "https://github.com/antaresbr/php-foundation",
    "keywords": [
        "php",
        "antaresbr",
        "foundation"
    ],
    "authors": [
        {
            "name": "Eduardo Guerra",
            "email": "eduardo.antares@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8",
        "voku/portable-ascii": "*"
    },
    "autoload": {
        "files": [
            "support/helpers.php"
        ],
        "psr-4": {
            "Antares\\Foundation\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12"
    },
    "autoload-dev": {
        "psr-4": {
            "Antares\\Foundation\\Tests\\": "tests/"
        }
    }
}
